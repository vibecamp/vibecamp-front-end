{
  "imports": {
    "bcrypt": "https://deno.land/x/bcrypt@v0.4.1/mod.ts",
    "djwts": "https://deno.land/x/djwt@v2.8/mod.ts",
    "postgres": "https://deno.land/x/postgres@v0.17.0/mod.ts",
    "oak": "https://deno.land/x/oak@v11.1.0/mod.ts",
    "std/": "https://deno.land/std@0.200.0/",
    "stripe": "npm:stripe@^13.5.0"
  },
  "fmt": {
    "semiColons": false,
    "singleQuote": true
  },
  "tasks": {
    "start": "deno run --allow-net --allow-env --check index.ts",
    "dev": "deno run --allow-net --allow-env --allow-read index.ts",
    "db-generate-types": "deno run --allow-all ./db-generate-types.ts"
  },
  "lock": false,
  "compilerOptions": {
    "strict": true,
    "noUncheckedIndexedAccess": true
  }
}